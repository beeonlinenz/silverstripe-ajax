{
    "name": "markguinn/silverstripe-ajax",
    "description": "Foundational ajax support for Silverstripe CMS: push/pull server-side regions, trigger client-side events, modals, status messages.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "ajax",
        "javascript",
        "events",
        "regions"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Guinn",
            "email": "mark@adaircreative.com",
            "homepage": "http://github.com/markguinn"
        }
    ],
    "extra": {
        "installer-name": "ajax",
        "expose": [
            "client"
        ]
    },
    "require": {
        "silverstripe/framework": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "MarkQuinn\\SilverStripeAjax\\": "src/",
            "MarkQuinn\\SilverStripeAjax\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
